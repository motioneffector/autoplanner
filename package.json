{
  "name": "@motioneffector/autoplanner",
  "version": "1.0.0",
  "description": "Autoplanner library",
  "author": "motioneffector",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motioneffector/autoplanner"
  },
  "homepage": "https://github.com/motioneffector/autoplanner#readme",
  "bugs": {
    "url": "https://github.com/motioneffector/autoplanner/issues"
  },
  "keywords": [
    "autoplanner",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:fuzz": "vitest run tests/fuzz",
    "test:fuzz:deep": "FUZZ_ITERATIONS=500 vitest run tests/fuzz",
    "test:fuzz:overnight": "FUZZ_ITERATIONS=2000 FUZZ_VERBOSE=true vitest run tests/fuzz",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/vitest-runner": "^9.5.1",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.6.2",
    "eslint": "^9.0.0",
    "fast-check": "^4.5.3",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
